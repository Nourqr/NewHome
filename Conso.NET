// get the pet's nickname. animalNickname can be blank.
do
{
    Console.WriteLine("Enter a nickname for the pet");
    readResult = Console.ReadLine();
    if (readResult != null)
    {
        animalNickname = readResult.ToLower();
        if (animalNickname == "")
        {
            animalNickname = "tbd";
        }
    }
} while (animalNickname == "");
